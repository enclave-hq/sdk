{
  "name": "@enclave-hq/sdk",
  "version": "2.0.2",
  "description": "Enclave SDK - Privacy-preserving multi-chain DeFi protocol client library",
  "keywords": [
    "enclave",
    "privacy",
    "zk-proof",
    "defi",
    "blockchain",
    "ethereum",
    "bsc",
    "sdk",
    "web3"
  ],
  "author": "Enclave Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/enclave-hq/sdk.git"
  },
  "homepage": "https://github.com/enclave-hq/sdk#readme",
  "bugs": {
    "url": "https://github.com/enclave-hq/sdk/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "import": "./dist/vue.mjs",
      "require": "./dist/vue.js"
    },
    "./nextjs": {
      "types": "./dist/nextjs.d.ts",
      "import": "./dist/nextjs.mjs",
      "require": "./dist/nextjs.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "build:prod": "NODE_ENV=production tsup",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "prepublishOnly": "npm run build:prod",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@enclave-hq/chain-utils": "^1.0.0",
    "mobx": "^6.12.0",
    "ethers": "^6.10.0",
    "axios": "^1.6.7",
    "eventemitter3": "^5.0.1"
  },
  "peerDependencies": {
    "ws": "^8.0.0",
    "react": ">=16.8.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "ws": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/vue": "^2.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitest/ui": "^1.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "mobx-react-lite": "^4.0.5",
    "prettier": "^3.2.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1",
    "vue": "^3.4.15"
  },
  "optionalDependencies": {
    "ws": "^8.16.0"
  },
  "browser": {
    "ws": false,
    "fs": false
  },
  "engines": {
    "node": ">=16.0.0"
  }
}

